{
  "name": "kodzitsu/queue",
  "description": "One-at-the time FIFO job queue mechanism with persistent queue log",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "KODZITSU",
      "email": "halo@kodzitsu.pl"
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "psr-4": {
      "Kodzitsu\\Queue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.34",
    "phpstan/phpstan": "^1.11",
    "squizlabs/php_codesniffer": "^3.10",
    "vimeo/psalm": "^5.24"
  }
}
